<script type="text/javascript">
function bottom_ad_banner_hide(){
    if($.trim($("#ad_cpt_12336_12338_12340_12342_12344_12346_12348_12350").html())==""){$("#blank_div").hide();return;}
    $("#ad_cpt_12336_12338_12340_12342_12344_12346_12348_12350").hide();
    $("#blank_div").hide();
    $("#bottom_ad_banner_show").show();        
    $("#bottom_ad_banner_show").click(function(){
         $("#ad_cpt_12336_12338_12340_12342_12344_12346_12348_12350").show();
         $("#blank_div").show();
         $("#bottom_ad_banner_show").hide();             
    });
}

    $(document).ready(function(){
    //µ×²¿¹ãžæ²Ù×÷¿ªÊŒ
    var  a =window.onload;
    if(typeof a == "function"){
        window.onload=function(){
            setTimeout("bottom_ad_banner_hide()",5000);
            a();
        }
    }
    else{
        window.onload=function(){
            setTimeout("bottom_ad_banner_hide()",5000);
        }
    }
    //µ×²¿¹ãžæ²Ù×÷œáÊø
    });


var jufunum=0;    
function showbanner(){
 var shownum = d_getCookies("showbanner");
 var showcookienum=0;     
var bigbannerhtml=$('#ad_cpt_12287').html();
var smallbannerhtml=$('#ad_cpt_12284').html();

if(bigbannerhtml==""||smallbannerhtml==""){    
    jufunum++;
    if(jufunum<3){
    setTimeout(showbanner,500);
    return;
    }
}
if(shownum!=""&&shownum!="NaN"){
    showcookienum=shownum;
}
if(showcookienum>=1||bigbannerhtml==""){
    if(smallbannerhtml!=''){
    $(document.body).prepend('<div id="__A_A" class="ad_bar_b ad_bar_top">'+smallbannerhtml+'</div>')
    $('#__A_A .ad_closeicon').click(function(){$('#__A_A').hide();});
    }
}else{
    $(document.body).prepend(TopAd(smallbannerhtml,bigbannerhtml));
    if(bigbannerhtml!=""){
        $("#__A_A_A").children().addClass("ad_bar_b");
        $("#__A_A_A").children().children().children()[0].onload=function(){topbannerclose=setTimeout("showImage();",7000);}
    }
    if(smallbannerhtml!=""){$("#adSmall .ad_closeicon").click(function(){$('#adSmall').hide();});$("#adSmall").removeClass("ddhome_topbanner");}
    }
    showipad();
}
 var showbannernum=0;
function createfd(){
    $.messager.lays(340, 256);
        var showbanner=$("#ad_cpt_12329").html();    
        if(showbanner!=""){
            $.messager.show(0, showbanner,5000);
            $("#ad_cpt_12329").html("");
        }else{
            showbannernum++;
            if(showbannernum<2){
                setTimeout(createfd,2000);
            }
        }
}
function openwindow(){
    var newwin_url1="";
    if(newwin_url1!=""){
        try{
            var newwin1=window.open (newwin_url1, "newwindow1", "height=100, width=100, toolbar=no, menubar=no, scrollbars=no, resizable=no, location=yes, status=yes, z-look=no, top=715, left=1500,titlebar=no  ");
            newwin1.blur();
        }catch(e){

        }

    }
    var newwin_url2="";
    if(newwin_url2!=""){
        try{
            var newwin2=window.open (newwin_url2, "newwindow2", "height=100, width=100, toolbar=no, menubar=no, scrollbars=no, resizable=no, location=yes, status=yes, z-look=no, top=715, left=1500,titlebar=no  ");
            newwin2.blur();
        }catch(e){

        }

    }
}
$(document).ready(function(){    
    setTimeout(showbanner,500);
    setTimeout(createfd,1000);   
    setTimeout(openwindow,5000);   
})



</script>
